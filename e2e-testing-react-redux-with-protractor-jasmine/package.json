{
  "name": "react-redux-app",
  "version": "1.0.0",
  "description": "react redux",
  "keywords": [
    "react",
    "redux",
    "es6",
    "webpack"
  ],
  "scripts": {
    "i:global": "npm i -g protractor && npm i -g webdriver-manager && npm i -g rimraf",
    "update:proxy-driver": "webdriver-manager --proxy http://genproxy.amdocs.com:8080 update --ignore_ssl && webdriver-manager --proxy http://genproxy.amdocs.com:8080 update --ie --ignore_ssl",
    "update:driver": "webdriver-manager update --ignore_ssl && webdriver-manager update --ie --ignore_ssl",
    "e2e:proxy-setup": "npm run i:global && npm run update:proxy-driver",
    "e2e:setup": "npm run i:global && npm run update:driver",
    "start:proxy-driver": "npm run update:proxy-driver && webdriver-manager --proxy http://genproxy.amdocs.com:8080 start --ignore_ssl",
    "start:driver": "npm run update:driver && webdriver-manager start --ignore_ssl",
    "start:server": "webpack-dev-server -w --inline --config webpack.dev.config.js",
    "remove:node": "rimraf ./node_modules",
    "remove:report": "rimraf ./__report__/",
    "test": "protractor ./tests/config.js",
    "sanity": "concurrently \"npm run start:server\" \"npm run test\"",
    "build": "webpack --progress"
  },
  "author": "Vikas sarkar",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "ffmpeg-binaries": "^4.0.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jquery": "^3.3.1",
    "protractor": "^5.4.0",
    "protractor-pretty-html-reporter": "0.0.16",
    "protractor-screenshoter-plugin": "^0.10.1",
    "protractor-video-reporter": "^0.3.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.0.6",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react-redux": "^5.0.7",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
